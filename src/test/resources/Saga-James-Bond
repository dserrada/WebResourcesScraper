<script type="text/javascript">

if(window.location.href.indexOf("dontorrents.one") == -1) {
       window.location = 'https://dontorrents.one/';
    }

    
    function post(path, params, method='post') {
        const form = document.createElement('form');
        form.method = method;
        form.action = path;

        for (const key in params) {
            if (params.hasOwnProperty(key)) {
                const hiddenField = document.createElement('input');
                hiddenField.type = 'hidden';
                hiddenField.name = key;
                hiddenField.value = params[key];

                form.appendChild(hiddenField);
            }
        }

        document.body.appendChild(form);
        form.submit();
    }
    
    $(function () {
        $('[data-toggle="popover"]').popover({
            html: true,
            sanitize: false
        })
    })


    $("[data-toggle='popover']").on('shown.bs.popover', () => {
        let copyBtns = document.querySelectorAll('.copyBtn');
        copyBtns.forEach(btn => {
            btn.addEventListener("click", () => {
                let holder = document.createElement("input");
                holder.style.position = 'absolute';
                holder.style.opacity = '0';
                document.body.appendChild(holder);

                holder.setAttribute("id", "holder_id");
                document.getElementById("holder_id").value = btn.dataset.clave;
                holder.select();

                try {
                    document.execCommand("copy");
                    document.body.removeChild(holder);
                    $(".toast").toast("show")
                } catch(e) {}
            });
        });
    })

if (typeof tipox !== 'undefined') { let tipox = document.querySelector("#tipox");

    tipox.addEventListener("change", event => {
        switch (event.target.value) {
            case "letra":
                document.querySelector("#letrasx").classList.remove("d-none");
                document.querySelector("#valor2x").classList.add("d-none");
                document.querySelector("#valorx").classList.add("d-none");
                break;
            case "genero":
                document.querySelector("#valor2x").classList.remove("d-none");
                document.querySelector("#letrasx").classList.add("d-none");
                document.querySelector("#valorx").classList.add("d-none");
                break;
            case "titulo":
            case "anyo":
            case "director":
            case "actores":
            case "contador_total":
            case "ultimos_dias":
                document.querySelector("#valorx").classList.remove("d-none");
                document.querySelector("#valor2x").classList.add("d-none");
                document.querySelector("#letrasx").classList.add("d-none");
                break;
            default:
                document.querySelector("#valor2x").classList.add("d-none");
                document.querySelector("#letrasx").classList.add("d-none");
                document.querySelector("#valorx").classList.add("d-none");

        }
    });}

</script>

</html>
